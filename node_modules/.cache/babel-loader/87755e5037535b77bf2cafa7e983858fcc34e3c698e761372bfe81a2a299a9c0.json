 }\r\n        &_yen {\r\n            font-size: 12px;\r\n        }\r\n        &_origin {\r\n            margin-left: 6px;\r\n            line-height: 2px;\r\n            font-size: 12px;\r\n            color: $light-fontColor;\r\n            text-decoration: line-through;\r\n        }\r\n        .product_number {\r\n            position: absolute;\r\n            right: 0;\r\n            bottom: 12px;\r\n            &_minus,\r\n            &_plus {\r\n                display: inline-block;\r\n                width: 20px;\r\n                height: 20px;\r\n                line-height: 16px;\r\n                border-radius: 50%;\r\n                font-size: 20px;\r\n                text-align: center;\r\n            }\r\n            &_minus {\r\n                border: 1px solid $medium-fontColor;\r\n                color: $medium-fontColor;\r\n                margin-right: 5px;\r\n            }\r\n            &_plus {\r\n                background: $btn-bgColor;\r\n                color: $bg_color;\r\n                margin-left: 5px;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AA+BA,SAASA,QAAQ,EAAEC,MAAK,QAAS,KAAI;AACrC,SAASC,GAAE,QAAS,qBAAoB;AACxC,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,KAAIA,CAAA,EAAK;IACP,MAAMC,IAAG,GAAIL,QAAQ,CAAC;MAAEM,WAAW,EAAE,CAAC,CAAC;IAAE,CAAC;IAC1C,MAAMC,cAAa,GAAI,MAAAA,CAAA,KAAY;MACjC,MAAMC,MAAK,GAAI,MAAMN,GAAG,CAAC,uBAAuB,EAAE;QAChDO,GAAG,EAAE;MACP,CAAC;MACDC,OAAO,CAACC,GAAG,CAACH,MAAM;IACpB;IACAD,cAAc,CAAC;IACf,MAAM;MAAED;IAAY,IAAIL,MAAM,CAACI,IAAI;IAEnC,OAAO;MAAEC;IAAY;EACvB;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}