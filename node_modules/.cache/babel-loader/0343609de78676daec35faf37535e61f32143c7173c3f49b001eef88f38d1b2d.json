{"ast":null,"code":"import { reactive, toRefs } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nimport { get } from '../../utils/request';\nimport ShopInfo from '../../components/Shopinfo';\nimport Content from './Content';\nimport Cart from './Cart';\nconst useShopInfoEffect = () => {\n  const route = useRoute();\n  const data = reactive({\n    item: {}\n  });\n  const getItemData = async () => {\n    // const result = await get(`/api/shop/${route.params.id}`)\n    const result = await get('/api/shop', {\n      id: route.params.id\n    });\n    console.log(result);\n    if (result?.data.errno === 0 && result?.data.data) {\n      data.item = result.data.data;\n    }\n  };\n  const {\n    item\n  } = toRefs(data);\n  return {\n    item,\n    getItemData\n  };\n};\nconst useBackRouterEffect = () => {\n  const router = useRouter();\n  const handleBackClik = () => {\n    router.back();\n  };\n  return handleBackClik;\n};\nexport default {\n  name: 'Shop',\n  components: {\n    ShopInfo,\n    Content,\n    Cart\n  },\n  setup() {\n    const {\n      item,\n      getItemData\n    } = useShopInfoEffect();\n    const handleBackClik = useBackRouterEffect();\n    getItemData();\n    return {\n      item,\n      handleBackClik\n    };\n  }\n};","map":{"version":3,"names":["reactive","toRefs","useRouter","useRoute","get","ShopInfo","Content","Cart","useShopInfoEffect","route","data","item","getItemData","result","id","params","console","log","errno","useBackRouterEffect","router","handleBackClik","back","name","components","setup"],"sources":["D:\\Besktop\\jingdong\\src\\views\\shop\\Shop.vue"],"sourcesContent":["<template>\r\n    <div class=\"wrapper\">\r\n        <div class=\"search\">\r\n            <div class=\"search_back iconfont\"\r\n            @click=\"handleBackClik\"\r\n            >&#xe6db;</div>\r\n            <div class=\"search_content\">\r\n                <spen class=\"search_content_icon iconfont\">&#xe644;</spen>\r\n                <input\r\n                class=\"search_content_input\"\r\n                placeholder=\"请输入商品名称\"\r\n                />\r\n            </div>\r\n        </div>\r\n        <div class=\"shop\">\r\n                 <img :src=\"item.imgUrl\" class=\"shop_img\">\r\n                 <div\r\n                 :class=\"{'shop_content':true, 'shop_content--bordered': history ? false: true}\"\r\n                 >\r\n                     <div class=\"shop_content_title\">{{ item.name }}</div>\r\n                     <div class=\"shop_content_tags\">\r\n                      <span  class=\"shop_content_tag\">月售：{{ item.sales }}</span>\r\n                      <span  class=\"shop_content_tag\">起送：{{ item.expressLimit }}</span>\r\n                      <span  class=\"shop_content_tag\">基础运费：{{ item.expressPrice }}</span>\r\n                     </div>\r\n                     <p class=\"shop_content_highlight\">{{ item.slogan }}</p>\r\n                 </div>\r\n              </div>    <Content :shopName=\"item.name\" />\r\n    <Cart />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs } from 'vue'\r\nimport { useRouter, useRoute } from 'vue-router'\r\nimport { get } from '../../utils/request'\r\nimport ShopInfo from '../../components/Shopinfo'\r\nimport Content from './Content'\r\nimport Cart from './Cart'\r\n\r\nconst useShopInfoEffect = () => {\r\n  const route = useRoute()\r\n  const data = reactive({ item: {} })\r\n  const getItemData = async () => {\r\n    // const result = await get(`/api/shop/${route.params.id}`)\r\n    const result = await get('/api/shop', { id: route.params.id })\r\n    console.log(result)\r\n    if (result?.data.errno === 0 && result?.data.data) {\r\n      data.item = result.data.data\r\n    }\r\n  }\r\n  const { item } = toRefs(data)\r\n  return { item, getItemData }\r\n}\r\n\r\nconst useBackRouterEffect = () => {\r\n  const router = useRouter()\r\n  const handleBackClik = () => {\r\n    router.back()\r\n  }\r\n  return handleBackClik\r\n}\r\n\r\nexport default {\r\n  name: 'Shop',\r\n  components: { ShopInfo, Content, Cart },\r\n  setup () {\r\n    const { item, getItemData } = useShopInfoEffect()\r\n    const handleBackClik = useBackRouterEffect()\r\n\r\n    getItemData()\r\n\r\n    return { item, handleBackClik }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n.wrapper {\r\n    padding: 4px 18px;\r\n}\r\n.search {\r\n    line-height: 32px;\r\n    display: flex;\r\n    margin: 2px 0 16px 0;\r\n    &_back {\r\n        width: 32px;\r\n        font-size: 24px;\r\n        color: #b6b6b6;\r\n    }\r\n    &_content {\r\n        display: flex;\r\n        flex: 1;\r\n        line-height: 32px;\r\n        background: $search-bgColor;\r\n        border-radius: 16px;\r\n        &_icon {\r\n            width: 44px;\r\n            text-align: center;\r\n            color: $search-fontcolor;\r\n        }\r\n        &_input {\r\n            display: block;\r\n            width: 100%;\r\n            padding-right: 5px;\r\n            border: none;\r\n            outline: none;\r\n            background: none;\r\n            height: 32px;\r\n            font-size: 14px;\r\n            color: $content-fontcolor;\r\n            &::placeholder {\r\n                color: $content-fontcolor;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AAiCA,SAASA,QAAQ,EAAEC,MAAK,QAAS,KAAI;AACrC,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAW;AAC/C,SAASC,GAAE,QAAS,qBAAoB;AACxC,OAAOC,QAAO,MAAO,2BAA0B;AAC/C,OAAOC,OAAM,MAAO,WAAU;AAC9B,OAAOC,IAAG,MAAO,QAAO;AAExB,MAAMC,iBAAgB,GAAIA,CAAA,KAAM;EAC9B,MAAMC,KAAI,GAAIN,QAAQ,CAAC;EACvB,MAAMO,IAAG,GAAIV,QAAQ,CAAC;IAAEW,IAAI,EAAE,CAAC;EAAE,CAAC;EAClC,MAAMC,WAAU,GAAI,MAAAA,CAAA,KAAY;IAC9B;IACA,MAAMC,MAAK,GAAI,MAAMT,GAAG,CAAC,WAAW,EAAE;MAAEU,EAAE,EAAEL,KAAK,CAACM,MAAM,CAACD;IAAG,CAAC;IAC7DE,OAAO,CAACC,GAAG,CAACJ,MAAM;IAClB,IAAIA,MAAM,EAAEH,IAAI,CAACQ,KAAI,KAAM,KAAKL,MAAM,EAAEH,IAAI,CAACA,IAAI,EAAE;MACjDA,IAAI,CAACC,IAAG,GAAIE,MAA