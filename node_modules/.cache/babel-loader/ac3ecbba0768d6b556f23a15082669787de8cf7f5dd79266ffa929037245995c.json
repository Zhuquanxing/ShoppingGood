{"ast":null,"code":"import { useRoute } from 'vue-router';\nimport { useCommonCartEffect } from '../../effects/cartEffects';\nimport TopArea from './TopArea';\nexport default {\n  name: 'OrderConfirmation',\n  components: {\n    TopArea\n  },\n  setup() {\n    const rotate = useRoute();\n    const shopId = rotate.params.id;\n    const {\n      shopName,\n      calculations,\n      productList\n    } = useCommonCartEffect(shopId);\n    return {\n      productList,\n      shopName,\n      calculations\n    };\n  }\n};","map":{"version":3,"names":["useRoute","useCommonCartEffect","TopArea","name","components","setup","rotate","shopId","params","id","shopName","calculations","productList"],"sources":["D:\\Besktop\\jingdong\\src\\views\\orderConfirmation\\OrderConfirmation.vue"],"sourcesContent":["<template>\r\n<div class=\"wrapper\">\r\n<TopArea />\r\n    <div class=\"products\">\r\n        <div class=\"products_title\">\r\n            {{ shopName }}\r\n        </div>\r\n        <div class=\"products_wrapper\">\r\n        <div class=\"products_list\">\r\n            <template\r\n            v-for=\"item in productList\"\r\n            :key=\"item.id\">\r\n            <div\r\n            v-if=\"item.count > 0\" class=\"products_item\"\r\n            >\r\n            <img class=\"products_item_img\" :src=\"item.imgUrl\" />\r\n            <div class=\"products_item_detail\">\r\n                <h4 class=\"products_item_title\">{{item.name}}</h4>\r\n                <p class=\"products_item_price\">\r\n                    <spen>\r\n                        <span class=\"products_item_yen\">&yen;</span>\r\n                        {{item.price}} x {{ item.count }}\r\n                    </spen>\r\n                    <spen class=\"products_item_total\">\r\n                        <span class=\"products_item_yen\">&yen;</span>\r\n                        {{(item.price * item.count).toFixed(2)}}\r\n                    </spen>\r\n                </p>\r\n            </div>\r\n          </div>\r\n        </template>\r\n        </div>\r\n    </div>\r\n    </div>\r\n    <div class=\"order\">\r\n        <div class=\"order_price\">实付金额 <b>{{ calculations.price }}</b></div>\r\n        <div class=\"order_btn\">提交订单</div>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport { useRoute } from 'vue-router'\r\nimport { useCommonCartEffect } from '../../effects/cartEffects'\r\nimport TopArea from './TopArea'\r\nexport default {\r\n  name: 'OrderConfirmation',\r\n  components: {\r\n    TopArea\r\n  },\r\n  setup () {\r\n    const rotate = useRoute()\r\n    const shopId = rotate.params.id\r\n    const { shopName, calculations, productList } = useCommonCartEffect(shopId)\r\n    return {\r\n      productList,\r\n      shopName,\r\n      calculations\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables';\r\n@import '../../style/mixins.scss';\r\n.wrapper {\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n    top: 0;\r\n    bottom: 0;\r\n    background-color: #eee;\r\n    overflow-y: scroll;\r\n}\r\n\r\n.products {\r\n    margin: 16px 18px 10px 18px;\r\n    background: #fff;\r\n    &_title {\r\n        padding: 16px;\r\n        font-size: 16px;\r\n        color: #333;\r\n    }\r\n    &_wrapper {\r\n        overflow-y: scroll;\r\n        margin: 0 18px;\r\n        position: absolute;\r\n        left: 0;\r\n        right: 0;\r\n        bottom: 60px;\r\n        top: 260px;\r\n    }\r\n    &_list {\r\n        background: #fff;\r\n    }\r\n    &_item {\r\n        position: relative;\r\n        display: flex;\r\n        padding: 0 16px 16px 16px;\r\n        &_img {\r\n            width: 46px;\r\n            height: 46px;\r\n            margin-right: 16px;\r\n        }\r\n        &_detail {\r\n            flex: 1;\r\n        }\r\n        &_title {\r\n            line-height: 2px;\r\n            font-size: 14px;\r\n            color: $content-fontcolor;\r\n            @include ellipsis;\r\n        }\r\n        &_price {\r\n            display: flex;\r\n            margin: 6px 0 0 0;\r\n            line-height: 20px;\r\n            font-size: 14px;\r\n            color: $highlight-fontColor;\r\n        }\r\n        &_total {\r\n            flex: 1;\r\n            text-align: right;\r\n            color: #000;\r\n        }\r\n        &_yen {\r\n            font-size: 12px;\r\n        }\r\n     }\r\n}\r\n\r\n.order {\r\n    position: absolute;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    display: flex;\r\n    height: 49px;\r\n    line-height: 49px;\r\n    background: #fff;\r\n    &_price {\r\n        flex: 1;\r\n        text-indent: 24px;\r\n        font-size: 14px;\r\n        color: #333;\r\n    }\r\n    &_btn {\r\n        width: 98px;\r\n        background: #4fb0f9;\r\n        color: #fff;\r\n        text-align: center;\r\n        font-size: 42px;\r\n    }\r\n}\r\n\r\n</style>\r\n"],"mappings":"AA0CA,SAASA,QAAO,QAAS,YAAW;AACpC,SAASC,mBAAkB,QAAS,2BAA0B;AAC9D,OAAOC,OAAM,MAAO,WAAU;AAC9B,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAIA,CAAA,EAAK;IACP,MAAMC,MAAK,GAAIN,QAAQ,CAAC;IACxB,MAAMO,MAAK,GAAID,MAAM,CAACE,MAAM,CAACC,EAAC;IAC9B,MAAM;MAAEC,QAAQ;MAAEC,YAAY;MAAEC;IAAY,IAAIX,mBAAmB,CAACM,MAAM;IAC1E,OAAO;MACLK,WAAW;MACXF,QAAQ;MACRC;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}