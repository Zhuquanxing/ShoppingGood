ick \">立即注册</div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { useRouter } from 'vue-router'\r\nimport axios from 'axios'\r\nimport { reactive } from 'vue'\r\n\r\naxios.defaults.headers.post['Content-Type'] = 'application/json'\r\n\r\nexport default {\r\n  name: 'Login',\r\n  setup () {\r\n    const date = reactive({\r\n      username: '',\r\n      password: ''\r\n    })\r\n    const router = useRouter()\r\n    const handleLogin = () => {\r\n      localStorage.isLogin = true\r\n      router.push({ name: 'Home' })\r\n      /* \r\n      axios.post('https://www.fastmock.site/mock/ae8e9031947a302fed5f92425995aa19/jd/api/user/login', {\r\n        username: 'date.username',\r\n        password: 'date.password'\r\n      }).then(() => {\r\n        localStorage.isLogin = true\r\n        router.push({ name: 'Home' })\r\n      }).catch(() => {\r\n        alert('登录失败')\r\n      }) */\r\n    }\r\n    const handleRegisterClick = () => {\r\n      router.push({ name: 'Register' })\r\n    }\r\n    return { handleLogin, handleRegisterClick, date }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n.wrapper {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 0;\r\n    right: 0;\r\n    transform: translateY(-50%);\r\n    &__img {\r\n        display: block;\r\n        margin: 0 auto 30px auto;\r\n        width: 66px;\r\n        height: 66px;\r\n    }\r\n    &__input {\r\n        height: 48px;\r\n        margin: 0 35px 16px 35px;\r\n        padding: 0 16px;\r\n        background: #f9f9f9;\r\n        border: 1px solid rgba(0,0,0,0.10);\r\n        border-radius: 6px;\r\n        border-radius: 6px;\r\n        &__content {\r\n            line-height: 48px;\r\n            border: none;\r\n            outline: none;\r\n            width: 100%;\r\n            background: none;\r\n            font-size: 16px;\r\n            color: $content-notice-fontcolor;\r\n            &::placeholder {\r\n                color: $content-notice-fontcolor;\r\n            }\r\n        }\r\n    }\r\n    &__login-button {\r\n        margin: 32px 35px 16px 35px;\r\n        line-height: 48px;\r\n        background: #0091ff;\r\n        box-shadow: 0 4px 8px 0 rgba(0,145,255,0.32);\r\n        border-radius: 4px;\r\n        border-radius: 4px;\r\n        color: #fff;\r\n        font-size: 16px;\r\n        text-align: center;\r\n    }\r\n    &__login-link {\r\n        text-align: center;\r\n        font-size: 14px;\r\n        color: $content-notice-fontcolor;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":";AAeA,SAASA,SAAQ,QAAS,YAAW;AACrC,OAAOC,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,KAAI;AAE7BD,KAAK,CAACE,QAAQ,CAACC,OAAO,CAACC,IAAI,CAAC,cAAc,IAAI,kBAAiB;AAE/D,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAIA,CAAA,EAAK;IACP,MAAMC,IAAG,GAAIN,QAAQ,CAAC;MACpBO,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;IACD,MAAMC,MAAK,GAAIX,SAAS,CAAC;IACzB,MAAMY,WAAU,GAAIA,CAAA,KAAM;MACxBC,YAAY,CAACC,OAAM,GAAI,IAAG;MAC1BH,MAAM,CAACI,IAAI,CAAC;QAAET,IAAI,EAAE;MAAO,CAAC;MAC5B;;;;;;;;;;IAUF;IACA,MAAMU,mBAAkB,GAAIA,CAAA,KAAM;MAChCL,MAAM,CAACI,IAAI,CAAC;QAAET,IAAI,EAAE;MAAW,CAAC;IAClC;IACA,OAAO;MAAEM,WAAW;MAAEI,mBAAmB;MAAER;IAAK;EAClD;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}