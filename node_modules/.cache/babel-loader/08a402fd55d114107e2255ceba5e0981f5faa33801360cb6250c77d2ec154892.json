 }\r\n      } catch (e) {\r\n        showToast('请求失败')\r\n      }\r\n\r\n\r\n    /*\r\n      axios.post('https://mock.apipark.cn/m1/4068509-0-default/api/user/login', {\r\n        username: 'date.username',\r\n        password: 'date.password'\r\n      }).then(() => {\r\n        localStorage.isLogin = true\r\n        router.push({ name: 'Home' })\r\n      }).catch(() => {\r\n        alert('登录失败')\r\n      })\r\n    */\r\n    }\r\n    const handleRegisterClick = () => {\r\n      router.push({ name: 'Register' })\r\n    }\r\n    return { handleLogin, handleRegisterClick, data, toastData }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n.wrapper {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 0;\r\n    right: 0;\r\n    transform: translateY(-50%);\r\n    &__img {\r\n        display: block;\r\n        margin: 0 auto 30px auto;\r\n        width: 66px;\r\n        height: 66px;\r\n    }\r\n    &__input {\r\n        height: 48px;\r\n        margin: 0 35px 16px 35px;\r\n        padding: 0 16px;\r\n        background: #f9f9f9;\r\n        border: 1px solid rgba(0,0,0,0.10);\r\n        border-radius: 6px;\r\n        border-radius: 6px;\r\n        &__content {\r\n            line-height: 48px;\r\n            border: none;\r\n            outline: none;\r\n            width: 100%;\r\n            background: none;\r\n            font-size: 16px;\r\n            color: $content-notice-fontcolor;\r\n            &::placeholder {\r\n                color: $content-notice-fontcolor;\r\n            }\r\n        }\r\n    }\r\n    &__login-button {\r\n        margin: 32px 35px 16px 35px;\r\n        line-height: 48px;\r\n        background: #0091ff;\r\n        box-shadow: 0 4px 8px 0 rgba(0,145,255,0.32);\r\n        border-radius: 4px;\r\n        border-radius: 4px;\r\n        color: #fff;\r\n        font-size: 16px;\r\n        text-align: center;\r\n    }\r\n    &__login-link {\r\n        text-align: center;\r\n        font-size: 14px;\r\n        color: $content-notice-fontcolor;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":";AAiBA,SAASA,QAAO,QAAS,KAAI;AAC7B,SAASC,SAAQ,QAAS,YAAW;AACrC,SAASC,IAAG,QAAS,qBAAoB;AACzC,OAAOC,KAAK,IAAIC,cAAa,QAAS,wBAAuB;AAC7D,OAAOC,KAAI,MAAO,OAAM;AAExBA,KAAK,CAACC,QAAQ,CAACC,OAAO,CAACL,IAAI,CAAC,cAAc,IAAI,kBAAiB;AAE/D,eAAe;EACbM,IAAI,EAAE,OAAO;EACbC,UAAU,EAAE;IAAEN;EAAM,CAAC;EACrBO,KAAIA,CAAA,EAAK;IACP,MAAMC,MAAK,GAAIV,SAAS,CAAC;IACzB,MAAMW,IAAG,GAAIZ,QAAQ,CAAC;MAAEa,QAAQ,EAAE,EAAE;MAAEC,QAAQ,EAAE;IAAG,CAAC;IACpD,MAAM;MAAEC,SAAS;MAAEC;IAAU,IAAIZ,cAAc,CAAC;IAEhD,MAAMa,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC9B,IAAI;QACF,MAAMC,MAAK,GAAI,MAAMhB,IAAI,CAAC,iBAAiB,EAAE;UAC3CW,QAAQ,EAAE,eAAe;UACzBC,QAAQ,EAAE;QACZ,CAAC;QACDK,YAAY,CAACC,OAAM,GAAI,IAAG;QAC1BT,MAAM,CAACU,IAAI,CAAC;UAAEb,IAAI,EAAE;QAAO,CAAC;QAC5Bc,KAAK,CAACJ,MAAM;QACZ,IAAIA,MAAM,EAAEK,KAAI,KAAM,CAAC,EAAE;UACvBJ,YAAY,CAACC,OAAM,GAAI,IAAG;UAC1BT,MAAM,CAACU,IAAI,CAAC;YAAEb,IAAI,EAAE;UAAO,CAAC;QAC9B,OAAO;UACLQ,SAAS,CAAC,MAAM;QAClB;MACF,EAAE,OAAOQ,CAAC,EAAE;QACVR,SAAS,CAAC,MAAM;MAClB;;MAGF;;;;;;;;;;;IAWA;IACA,MAAMS,mBAAkB,GAAIA,CAAA,KAAM;MAChCd,MAAM,CAACU,IAAI,CAAC;QAAEb,IAAI,EAAE;MAAW,CAAC;IAClC;IACA,OAAO;MAAES,WAAW;MAAEQ,mBAAmB;MAAEb,IAAI;MAAEG;IAAU;EAC7D;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}