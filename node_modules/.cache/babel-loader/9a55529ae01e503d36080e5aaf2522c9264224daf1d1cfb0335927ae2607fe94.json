{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute } from 'vue-router';\nconst useCartEffect = () => {\n  const store = useStore();\n  const route = useRoute();\n  const shopId = route.params.id;\n  const cartList = store.state.cartList;\n  const total = computed(() => {\n    const productList = cartList[shopId];\n    let count = 0;\n    if (productList) {\n      for (const i in productList) {\n        const product = productList[i];\n        count += product.count;\n      }\n    }\n    return count;\n  });\n  const price = computed(() => {\n    const productList = cartList[shopId];\n    let count = 0;\n    if (productList) {\n      for (const i in productList) {\n        const product = productList[i];\n        count += product.count * product.price;\n      }\n    }\n    return count.toFixed(2);\n  });\n  const productList = computed(() => {\n    const productList = cartList[shopId] || [];\n    return productList;\n  });\n  return {\n    total,\n    price,\n    productList\n  };\n};\nexport default {\n  name: 'Cart',\n  setup() {\n    const {\n      total,\n      price,\n      productList\n    } = useCartEffect();\n    return {\n      total,\n      price,\n      productList\n    };\n  }\n};","map":{"version":3,"names":["computed","useStore","useRoute","useCartEffect","store","route","shopId","params","id","cartList","state","total","productList","count","i","product","price","toFixed","name","setup"],"sources":["D:\\Besktop\\jingdong\\src\\views\\shop\\Cart.vue"],"sourcesContent":["<template>\r\n    <div class=\"cart\">\r\n        <div class=\"product\">\r\n            <div class=\"product_item\"\r\n          v-for=\"item in productList\"\r\n          :key=\"item._id\">\r\n            <img class=\"product_item_img\" :src=\"item.imgUrl\" alt=\"\">\r\n            <div class=\"product_item_detail\">\r\n                <h4 class=\"product_item_title\">{{item.name}}</h4>\r\n                <p class=\"product_item_price\">\r\n                    <span class=\"product_item_yen\">&yen;</span>{{item.price}}\r\n                    <span class=\"product_item_origin\">&yen;{{ item.oldPrice }}</span>\r\n                </p>\r\n            </div>\r\n            <div class=\"product_number\">\r\n                <span class=\"product_number_minus\"\r\n                @click=\"() => {changeCartItemInfo(shopId, item._id, item, -1)}\"\r\n                >-</span>\r\n                  {{ cartList?.[shopId]?.[item._id]?.count || 0 }}\r\n                <span class=\"product_number_plus\"\r\n                @click=\"() => {changeCartItemInfo(shopId, item._id, item, 1)}\"\r\n                >+</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n       <div class=\"check\">\r\n           <div class=\"icon\">\r\n               <img src=\"http://www.dell-lee.com/imgs/vue3/tomato.png\"\r\n               class=\"check_icon_img\"\r\n               />\r\n               <div class=\"check_icon_tag\">{{ total }}</div>\r\n           </div>\r\n           <div class=\"check_info\">\r\n              总计：<span class=\"check_info_price\">&yen; {{price}}</span>\r\n           </div>\r\n           <div class=\"check_btn\">去结算</div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue'\r\nimport { useStore } from 'vuex'\r\nimport { useRoute } from 'vue-router'\r\n\r\nconst useCartEffect = () => {\r\n  const store = useStore()\r\n  const route = useRoute()\r\n  const shopId = route.params.id\r\n  const cartList = store.state.cartList\r\n  const total = computed(() => {\r\n    const productList = cartList[shopId]\r\n    let count = 0\r\n    if (productList) {\r\n      for (const i in productList) {\r\n        const product = productList[i]\r\n        count += product.count\r\n      }\r\n    }\r\n    return count\r\n  })\r\n  const price = computed(() => {\r\n    const productList = cartList[shopId]\r\n    let count = 0\r\n    if (productList) {\r\n      for (const i in productList) {\r\n        const product = productList[i]\r\n        count += (product.count * product.price)\r\n      }\r\n    }\r\n    return count.toFixed(2)\r\n  })\r\n\r\n  const productList = computed(() => {\r\n    const productList = cartList[shopId] || []\r\n    return productList\r\n  })\r\n\r\n  return { total, price, productList }\r\n}\r\n\r\nexport default {\r\n  name: 'Cart',\r\n  setup () {\r\n    const { total, price, productList } = useCartEffect()\r\n    return { total, price, productList }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables';\r\n@import '../../style/mixins.scss';\r\n.cart {\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n}\r\n.product {\r\n    overflow-y: scroll;\r\n    flex: 1;\r\n    &_item {\r\n        position: relative;\r\n        display: flex;\r\n        padding: 12px 0;\r\n        margin: 0 16px;\r\n        border-bottom: 1px solid $content-bgColor;\r\n        &_detail {\r\n            overflow: hidden;\r\n        }\r\n        &_img {\r\n            width: 75px;\r\n            height: 75px;\r\n            margin-right: 16px;\r\n        }\r\n        &_title {\r\n            line-height: 2px;\r\n            font-size: 14px;\r\n            color: $content-fontcolor;\r\n            @include ellipsis;\r\n        }\r\n        &_sales {\r\n            margin: 6px 0;\r\n            font-size: 12px;\r\n            color: $content-fontcolor;\r\n        }\r\n        &_price {\r\n            margin: 0;\r\n            line-height: 20px;\r\n            font-size: 14px;\r\n            color: $highlight-fontColor;\r\n        }\r\n        &_yen {\r\n            font-size: 12px;\r\n        }\r\n        &_origin {\r\n            margin-left: 6px;\r\n            line-height: 2px;\r\n            font-size: 12px;\r\n            color: $light-fontColor;\r\n            text-decoration: line-through;\r\n        }\r\n        .product_number {\r\n            position: absolute;\r\n            right: 0;\r\n            bottom: 12px;\r\n            &_minus,\r\n            &_plus {\r\n                display: inline-block;\r\n                width: 20px;\r\n                height: 20px;\r\n                line-height: 16px;\r\n                border-radius: 50%;\r\n                font-size: 20px;\r\n                text-align: center;\r\n            }\r\n            &_minus {\r\n                border: 1px solid $medium-fontColor;\r\n                color: $medium-fontColor;\r\n                margin-right: 5px;\r\n            }\r\n            &_plus {\r\n                background: $btn-bgColor;\r\n                color: $bg_color;\r\n                margin-left: 5px;\r\n            }\r\n        }\r\n    }\r\n}\r\n.check {\r\n    display: flex;\r\n    height: 49px;\r\n    border-top: 1px solid $content-bgColor;\r\n    line-height: 49px;\r\n    &_icon {\r\n        position: relative;\r\n        width: 84px;\r\n        &_img {\r\n            display: block;\r\n            margin: 12px auto;\r\n            width: 28px;\r\n            height: 26px;\r\n        }\r\n        &_tag {\r\n            position: absolute;\r\n            left: 20px;\r\n            top: 4px;\r\n            padding: 0 4px;\r\n            min-width: 20px;\r\n            height: 20px;\r\n            line-height: 20px;\r\n            background-color: $highlight-fontColor;\r\n            border-radius: 10px;\r\n            font-size: 12px;\r\n            text-align: center;\r\n            color: $bg_color;\r\n            transform: scale(.5);\r\n            transform-origin: left center;\r\n        }\r\n    }\r\n    &_info {\r\n        flex: 1;\r\n        font-size: 12px;\r\n        color: $content-fontcolor;\r\n        &_price {\r\n            line-height: 49px;\r\n            color: $highlight-fontColor;\r\n            font-size: 18px;\r\n        }\r\n    }\r\n    &_btn {\r\n        width: 98px;\r\n        background-color: #4fb0f9;\r\n        text-align: center;\r\n        color: $bg_color;\r\n        font-size: 14px;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AAyCA,SAASA,QAAO,QAAS,KAAI;AAC7B,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,QAAO,QAAS,YAAW;AAEpC,MAAMC,aAAY,GAAIA,CAAA,KAAM;EAC1B,MAAMC,KAAI,GAAIH,QAAQ,CAAC;EACvB,MAAMI,KAAI,GAAIH,QAAQ,CAAC;EACvB,MAAMI,MAAK,GAAID,KAAK,CAACE,MAAM,CAACC,EAAC;EAC7B,MAAMC,QAAO,GAAIL,KAAK,CAACM,KAAK,CAACD,QAAO;EACpC,MAAME,KAAI,GAAIX,QAAQ,CAAC,MAAM;IAC3B,MAAMY,WAAU,GAAIH,QAAQ,CAACH,MAAM;IACnC,IAAIO,KAAI,GAAI;IACZ,IAAID,WAAW,EAAE;MACf,KAAK,MAAME,CAAA,IAAKF,WAAW,EAAE;QAC3B,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAC;QAC7BD,KAAI,IAAKE,OAAO,CAACF,KAAI;MACvB;IACF;IACA,OAAOA,KAAI;EACb,CAAC;EACD,MAAMG,KAAI,GAAIhB,QAAQ,CAAC,MAAM;IAC3B,MAAMY,WAAU,GAAIH,QAAQ,CAACH,MAAM;IACnC,IAAIO,KAAI,GAAI;IACZ,IAAID,WAAW,EAAE;MACf,KAAK,MAAME,CAAA,IAAKF,WAAW,EAAE;QAC3B,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAC;QAC7BD,KAAI,IAAME,OAAO,CAACF,KAAI,GAAIE,OAAO,CAACC,KAAK;MACzC;IACF;IACA,OAAOH,KAAK,CAACI,OAAO,CAAC,CAAC;EACxB,CAAC;EAED,MAAML,WAAU,GAAIZ,QAAQ,CAAC,MAAM;IACjC,MAAMY,WAAU,GAAIH,QAAQ,CAACH,MAAM,KAAK,EAAC;IACzC,OAAOM,WAAU;EACnB,CAAC;EAED,OAAO;IAAED,KAAK;IAAEK,KAAK;IAAEJ;EAAY;AACrC;AAEA,eAAe;EACbM,IAAI,EAAE,MAAM;EACZC,KAAIA,CAAA,EAAK;IACP,MAAM;MAAER,KAAK;MAAEK,KAAK;MAAEJ;IAAY,IAAIT,aAAa,CAAC;IACpD,OAAO;MAAEQ,KAAK;MAAEK,KAAK;MAAEJ;IAAY;EACrC;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}