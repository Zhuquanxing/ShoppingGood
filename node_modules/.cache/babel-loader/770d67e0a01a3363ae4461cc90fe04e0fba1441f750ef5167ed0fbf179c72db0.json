{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute } from 'vue-router';\nimport { useCommonCartEffect } from './commonCartEffect';\nconst useCartEffect = shopId => {\n  const {\n    changeCartItemInfo\n  } = useCommonCartEffect();\n  const store = useStore();\n  const cartList = store.state.cartList;\n  const total = computed(() => {\n    const productList = cartList[shopId];\n    let count = 0;\n    if (productList) {\n      for (const i in productList) {\n        const product = productList[i];\n        count += product.count;\n      }\n    }\n    return count;\n  });\n  const price = computed(() => {\n    const productList = cartList[shopId];\n    let count = 0;\n    if (productList) {\n      for (const i in productList) {\n        const product = productList[i];\n        if (product.checked) {\n          count += product.count * product.price;\n        }\n      }\n    }\n    return count.toFixed(2);\n  });\n  const productList = computed(() => {\n    const productList = cartList[shopId] || [];\n    return productList;\n  });\n  const changeCartItemChecked = (shopId, productId) => {\n    store.commit('changeCartItemChecked', {\n      shopId,\n      productId\n    });\n  };\n  const cleanCartProducts = shopId => {\n    store.commit('cleanCartProducts', {\n      shopId\n    });\n  };\n  return {\n    total,\n    price,\n    productList,\n    changeCartItemInfo,\n    changeCartItemChecked,\n    cleanCartProducts\n  };\n};\nexport default {\n  name: 'Cart',\n  setup() {\n    const route = useRoute();\n    const shopId = route.params.id;\n    const {\n      total,\n      price,\n      productList,\n      changeCartItemInfo,\n      changeCartItemChecked,\n      cleanCartProducts\n    } = useCartEffect(shopId);\n    return {\n      total,\n      price,\n      shopId,\n      productList,\n      changeCartItemInfo,\n      changeCartItemChecked,\n      cleanCartProducts\n    };\n  }\n};","map":{"version":3,"names":["computed","useStore","useRoute","useCommonCartEffect","useCartEffect","shopId","changeCartItemInfo","store","cartList","state","total","productList","count","i","product","price","checked","toFixed","changeCartItemChecked","productId","commit","cleanCartProducts","name","setup","route","params","id"],"sources":["D:\\Besktop\\jingdong\\src\\views\\shop\\Cart.vue"],"sourcesContent":["<template>\r\n    <div class=\"cart\">\r\n        <div class=\"product\">\r\n            <div class=\"product_header\">\r\n                <div class=\"product_header_all\">\r\n                    <span class=\"product_header_icon iconfont\">\r\n                        \r\n                    </span>\r\n                    全选</div>\r\n                <div class=\"product_header_clear\"\r\n                @click=\"() => cleanCartProducts(shopId)\"\r\n                >清空购物车</div>\r\n            </div>\r\n            <template\r\n          v-for=\"item in productList\"\r\n          :key=\"item._id\">\r\n          <div class=\"product_item\" v-if=\"item.count > 0\">\r\n            <div class=\"product_item_checked iconfont\"\r\n            v-html=\"item.checked ? '&#xe618;' : '&#xe6f7;'\"\r\n            @click.stop=\"() => changeCartItemChecked(shopId, item._id)\"\r\n            />\r\n            <img class=\"product_item_img\" :src=\"item.imgUrl\" alt=\"\">\r\n            <div class=\"product_item_detail\">\r\n                <h4 class=\"product_item_title\">{{item.name}}</h4>\r\n                <p class=\"product_item_price\">\r\n                    <span class=\"product_item_yen\">&yen;</span>{{item.price}}\r\n                    <span class=\"product_item_origin\">&yen;{{ item.oldPrice }}</span>\r\n                </p>\r\n            </div>\r\n            <div class=\"product_number\">\r\n                <span class=\"product_number_minus\"\r\n                @click=\"() => {changeCartItemInfo(shopId, item._id, item, -1)}\"\r\n                >-</span>\r\n                  {{ item.count || 0 }}\r\n                <span class=\"product_number_plus\"\r\n                @click=\"() => {changeCartItemInfo(shopId, item._id, item, 1)}\"\r\n                >+</span>\r\n            </div>\r\n            </div>\r\n            </template>\r\n        </div>\r\n       <div class=\"check\">\r\n           <div class=\"icon\">\r\n               <img src=\"http://www.dell-lee.com/imgs/vue3/tomato.png\"\r\n               class=\"check_icon_img\"\r\n               />\r\n               <div class=\"check_icon_tag\">{{ total }}</div>\r\n           </div>\r\n           <div class=\"check_info\">\r\n              总计：<span class=\"check_info_price\">&yen; {{price}}</span>\r\n           </div>\r\n           <div class=\"check_btn\">去结算</div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue'\r\nimport { useStore } from 'vuex'\r\nimport { useRoute } from 'vue-router'\r\nimport { useCommonCartEffect } from './commonCartEffect'\r\n\r\nconst useCartEffect = (shopId) => {\r\n  const { changeCartItemInfo } = useCommonCartEffect()\r\n  const store = useStore()\r\n  const cartList = store.state.cartList\r\n  const total = computed(() => {\r\n    const productList = cartList[shopId]\r\n    let count = 0\r\n    if (productList) {\r\n      for (const i in productList) {\r\n        const product = productList[i]\r\n        count += product.count\r\n      }\r\n    }\r\n    return count\r\n  })\r\n  const price = computed(() => {\r\n    const productList = cartList[shopId]\r\n    let count = 0\r\n    if (productList) {\r\n      for (const i in productList) {\r\n        const product = productList[i]\r\n        if (product.checked) {\r\n          count += (product.count * product.price)\r\n        }\r\n      }\r\n    }\r\n    return count.toFixed(2)\r\n  })\r\n\r\n  const productList = computed(() => {\r\n    const productList = cartList[shopId] || []\r\n    return productList\r\n  })\r\n\r\n  const changeCartItemChecked = (shopId, productId) => {\r\n    store.commit('changeCartItemChecked', { shopId, productId })\r\n  }\r\n\r\n  const cleanCartProducts = (shopId) => {\r\n    store.commit('cleanCartProducts', { shopId })\r\n  }\r\n\r\n  return { total, price, productList, changeCartItemInfo, changeCartItemChecked, cleanCartProducts }\r\n}\r\n\r\nexport default {\r\n  name: 'Cart',\r\n  setup () {\r\n    const route = useRoute()\r\n    const shopId = route.params.id\r\n    const { total, price, productList, changeCartItemInfo, changeCartItemChecked, cleanCartProducts } = useCartEffect(shopId)\r\n    return { total, price, shopId, productList, changeCartItemInfo, changeCartItemChecked, cleanCartProducts }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables';\r\n@import '../../style/mixins.scss';\r\n.cart {\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n}\r\n.product {\r\n    overflow-y: scroll;\r\n    flex: 1;\r\n    background: #fff;\r\n    &_header {\r\n        display: flex;\r\n        line-height: 52px;\r\n        border-bottom: 1px solid #f1f1f1;\r\n        &_all {\r\n            width: 64px;\r\n        }\r\n        &_clear {\r\n            flex: 1;\r\n            margin-right: 16px;\r\n            text-align: right;\r\n            font-size: 14px;\r\n            color: #333;\r\n        }\r\n    }\r\n    &_item {\r\n        position: relative;\r\n        display: flex;\r\n        padding: 12px 0;\r\n        margin: 0 16px;\r\n        border-bottom: 1px solid $content-bgColor;\r\n        &_checked {\r\n            line-height: 50px;\r\n            margin-right: 20px;\r\n            color: #0091ff;\r\n            font-size: 20px;\r\n        }\r\n        &_detail {\r\n            overflow: hidden;\r\n        }\r\n        &_img {\r\n            width: 46px;\r\n            height: 46px;\r\n            margin-right: 16px;\r\n        }\r\n        &_title {\r\n            line-height: 2px;\r\n            font-size: 14px;\r\n            color: $content-fontcolor;\r\n            @include ellipsis;\r\n        }\r\n        &_price {\r\n            margin: 6px 0 0 0;\r\n            line-height: 20px;\r\n            font-size: 14px;\r\n            color: $highlight-fontColor;\r\n        }\r\n        &_yen {\r\n            font-size: 12px;\r\n        }\r\n        &_origin {\r\n            margin-left: 6px;\r\n            line-height: 2px;\r\n            font-size: 12px;\r\n            color: $light-fontColor;\r\n            text-decoration: line-through;\r\n        }\r\n        .product_number {\r\n            position: absolute;\r\n            right: 0;\r\n            bottom: 12px;\r\n            &_minus,\r\n            &_plus {\r\n                display: inline-block;\r\n                width: 20px;\r\n                height: 20px;\r\n                line-height: 16px;\r\n                border-radius: 50%;\r\n                font-size: 20px;\r\n                text-align: center;\r\n            }\r\n            &_minus {\r\n                border: 1px solid $medium-fontColor;\r\n                color: $medium-fontColor;\r\n                margin-right: 5px;\r\n            }\r\n            &_plus {\r\n                background: $btn-bgColor;\r\n                color: $bg_color;\r\n                margin-left: 5px;\r\n            }\r\n        }\r\n    }\r\n}\r\n.check {\r\n    display: flex;\r\n    height: 49px;\r\n    border-top: 1px solid $content-bgColor;\r\n    line-height: 49px;\r\n    &_icon {\r\n        position: relative;\r\n        width: 84px;\r\n        &_img {\r\n            display: block;\r\n            margin: 12px auto;\r\n            width: 28px;\r\n            height: 26px;\r\n        }\r\n        &_tag {\r\n            position: absolute;\r\n            left: 20px;\r\n            top: 4px;\r\n            padding: 0 4px;\r\n            min-width: 20px;\r\n            height: 20px;\r\n            line-height: 20px;\r\n            background-color: $highlight-fontColor;\r\n            border-radius: 10px;\r\n            font-size: 12px;\r\n            text-align: center;\r\n            color: $bg_color;\r\n            transform: scale(.5);\r\n            transform-origin: left center;\r\n        }\r\n    }\r\n    &_info {\r\n        flex: 1;\r\n        font-size: 12px;\r\n        color: $content-fontcolor;\r\n        &_price {\r\n            line-height: 49px;\r\n            color: $highlight-fontColor;\r\n            font-size: 18px;\r\n        }\r\n    }\r\n    &_btn {\r\n        width: 98px;\r\n        background-color: #4fb0f9;\r\n        text-align: center;\r\n        color: $bg_color;\r\n        font-size: 14px;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AAyDA,SAASA,QAAO,QAAS,KAAI;AAC7B,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,QAAO,QAAS,YAAW;AACpC,SAASC,mBAAkB,QAAS,oBAAmB;AAEvD,MAAMC,aAAY,GAAKC,MAAM,IAAK;EAChC,MAAM;IAAEC;EAAmB,IAAIH,mBAAmB,CAAC;EACnD,MAAMI,KAAI,GAAIN,QAAQ,CAAC;EACvB,MAAMO,QAAO,GAAID,KAAK,CAACE,KAAK,CAACD,QAAO;EACpC,MAAME,KAAI,GAAIV,QAAQ,CAAC,MAAM;IAC3B,MAAMW,WAAU,GAAIH,QAAQ,CAACH,MAAM;IACnC,IAAIO,KAAI,GAAI;IACZ,IAAID,WAAW,EAAE;MACf,KAAK,MAAME,CAAA,IAAKF,WAAW,EAAE;QAC3B,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAC;QAC7BD,KAAI,IAAKE,OAAO,CAACF,KAAI;MACvB;IACF;IACA,OAAOA,KAAI;EACb,CAAC;EACD,MAAMG,KAAI,GAAIf,QAAQ,CAAC,MAAM;IAC3B,MAAMW,WAAU,GAAIH,QAAQ,CAACH,MAAM;IACnC,IAAIO,KAAI,GAAI;IACZ,IAAID,WAAW,EAAE;MACf,KAAK,MAAME,CAAA,IAAKF,WAAW,EAAE;QAC3B,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAC;QAC7B,IAAIC,OAAO,CAACE,OAAO,EAAE;UACnBJ,KAAI,IAAME,OAAO,CAACF,KAAI,GAAIE,OAAO,CAACC,KAAK;QACzC;MACF;IACF;IACA,OAAOH,KAAK,CAACK,OAAO,CAAC,CAAC;EACxB,CAAC;EAED,MAAMN,WAAU,GAAIX,QAAQ,CAAC,MAAM;IACjC,MAAMW,WAAU,GAAIH,QAAQ,CAACH,MAAM,KAAK,EAAC;IACzC,OAAOM,WAAU;EACnB,CAAC;EAED,MAAMO,qBAAoB,GAAIA,CAACb,MAAM,EAAEc,SAAS,KAAK;IACnDZ,KAAK,CAACa,MAAM,CAAC,uBAAuB,EAAE;MAAEf,MAAM;MAAEc;IAAU,CAAC;EAC7D;EAEA,MAAME,iBAAgB,GAAKhB,MAAM,IAAK;IACpCE,KAAK,CAACa,MAAM,CAAC,mBAAmB,EAAE;MAAEf;IAAO,CAAC;EAC9C;EAEA,OAAO;IAAEK,KAAK;IAAEK,KAAK;IAAEJ,WAAW;IAAEL,kBAAkB;IAAEY,qBAAqB;IAAEG;EAAkB;AACnG;AAE