{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute } from 'vue-router';\nconst useCartEffect = () => {\n  const store = useStore();\n  const route = useRoute();\n  const shopId = route.params.id;\n  const cartList = store.state.cartList;\n  const total = computed(() => {\n    const productList = cartList[shopId];\n    let count = 0;\n    if (productList) {\n      for (const i in productList) {\n        const product = productList[i];\n        count += product.count;\n      }\n    }\n    return count;\n  });\n  const price = computed(() => {\n    const productList = cartList[shopId];\n    let count = 0;\n    if (productList) {\n      for (const i in productList) {\n        const product = productList[i];\n        count += product.count * product.price;\n      }\n    }\n    return count.toFixed(2);\n  });\n};\nexport default {\n  name: 'Cart',\n  setup() {\n    return {\n      total,\n      price\n    };\n  }\n};","map":{"version":3,"names":["computed","useStore","useRoute","useCartEffect","store","route","shopId","params","id","cartList","state","total","productList","count","i","product","price","toFixed","name","setup"],"sources":["D:\\Besktop\\jingdong\\src\\views\\shop\\Cart.vue"],"sourcesContent":["<template>\r\n    <div class=\"cart\">\r\n       <div class=\"check\">\r\n           <div class=\"icon\">\r\n               <img src=\"http://www.dell-lee.com/imgs/vue3/tomato.png\"\r\n               class=\"check_icon_img\"\r\n               />\r\n               <div class=\"check_icon_tag\">{{ total }}</div>\r\n           </div>\r\n           <div class=\"check_info\">\r\n              总计：<span class=\"check_info_price\">&yen; {{price}}</span>\r\n           </div>\r\n           <div class=\"check_btn\">去结算</div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue'\r\nimport { useStore } from 'vuex'\r\nimport { useRoute } from 'vue-router'\r\n\r\nconst useCartEffect = () => {\r\n    const store = useStore()\r\n    const route = useRoute()\r\n    const shopId = route.params.id\r\n    const cartList = store.state.cartList\r\n  const total = computed(() => {\r\n    const productList = cartList[shopId]\r\n    let count = 0\r\n    if (productList) {\r\n      for (const i in productList) {\r\n        const product = productList[i]\r\n        count += product.count\r\n      }\r\n    }\r\n    return count\r\n  })\r\n  const price = computed(() => {\r\n    const productList = cartList[shopId]\r\n    let count = 0\r\n    if (productList) {\r\n      for (const i in productList) {\r\n        const product = productList[i]\r\n        count += (product.count * product.price)\r\n      }\r\n    }\r\n    return count.toFixed(2)\r\n  })\r\n}\r\n\r\nexport default {\r\n  name: 'Cart',\r\n  setup () {\r\n    return { total, price }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables';\r\n.cart {\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n\r\n}\r\n.check {\r\n    display: flex;\r\n    height: 49px;\r\n    border-top: 1px solid $content-bgColor;\r\n    line-height: 49px;\r\n    &_icon {\r\n        position: relative;\r\n        width: 84px;\r\n        &_img {\r\n            display: block;\r\n            margin: 12px auto;\r\n            width: 28px;\r\n            height: 26px;\r\n        }\r\n        &_tag {\r\n            position: absolute;\r\n            left: 20px;\r\n            top: 4px;\r\n            padding: 0 4px;\r\n            min-width: 20px;\r\n            height: 20px;\r\n            line-height: 20px;\r\n            background-color: $highlight-fontColor;\r\n            border-radius: 10px;\r\n            font-size: 12px;\r\n            text-align: center;\r\n            color: $bg_color;\r\n            transform: scale(.5);\r\n            transform-origin: left center;\r\n        }\r\n    }\r\n    &_info {\r\n        flex: 1;\r\n        font-size: 12px;\r\n        color: $content-fontcolor;\r\n        &_price {\r\n            line-height: 49px;\r\n            color: $highlight-fontColor;\r\n            font-size: 18px;\r\n        }\r\n    }\r\n    &_btn {\r\n        width: 98px;\r\n        background-color: #4fb0f9;\r\n        text-align: center;\r\n        color: $bg_color;\r\n        font-size: 14px;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AAkBA,SAASA,QAAO,QAAS,KAAI;AAC7B,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,QAAO,QAAS,YAAW;AAEpC,MAAMC,aAAY,GAAIA,CAAA,KAAM;EACxB,MAAMC,KAAI,GAAIH,QAAQ,CAAC;EACvB,MAAMI,KAAI,GAAIH,QAAQ,CAAC;EACvB,MAAMI,MAAK,GAAID,KAAK,CAACE,MAAM,CAACC,EAAC;EAC7B,MAAMC,QAAO,GAAIL,KAAK,CAACM,KAAK,CAACD,QAAO;EACtC,MAAME,KAAI,GAAIX,QAAQ,CAAC,MAAM;IAC3B,MAAMY,WAAU,GAAIH,QAAQ,CAACH,MAAM;IACnC,IAAIO,KAAI,GAAI;IACZ,IAAID,WAAW,EAAE;MACf,KAAK,MAAME,CAAA,IAAKF,WAAW,EAAE;QAC3B,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAC;QAC7BD,KAAI,IAAKE,OAAO,CAACF,KAAI;MACvB;IACF;IACA,OAAOA,KAAI;EACb,CAAC;EACD,MAAMG,KAAI,GAAIhB,QAAQ,CAAC,MAAM;IAC3B,MAAMY,WAAU,GAAIH,QAAQ,CAACH,MAAM;IACnC,IAAIO,KAAI,GAAI;IACZ,IAAID,WAAW,EAAE;MACf,KAAK,MAAME,CAAA,IAAKF,WAAW,EAAE;QAC3B,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAC;QAC7BD,KAAI,IAAME,OAAO,CAACF,KAAI,GAAIE,OAAO,CAACC,KAAK;MACzC;IACF;IACA,OAAOH,KAAK,CAACI,OAAO,CAAC,CAAC;EACxB,CAAC;AACH;AAEA,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,KAAIA,CAAA,EAAK;IACP,OAAO;MAAER,KAAK;MAAEK;IAAM;EACxB;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}