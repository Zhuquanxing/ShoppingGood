ontent)\r\n  return { list }\r\n}\r\n\r\nconst useCartEffect = () => {\r\n    const cart = ref(null)\r\n    return { cart }\r\n}\r\n\r\nexport default {\r\n  name: 'Content',\r\n  setup () {\r\n    const { currentTab, handleTabClick } = useTabEffect()\r\n    const { list } = useCurrentListEffect(currentTab)\r\n\r\n    // const { contentList } = toRefs(data)\r\n\r\n    return { categories, currentTab, handleTabClick, list }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables';\r\n@import '../../style/mixins.scss';\r\n.content {\r\n    display: flex;\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n    top: 15px;\r\n    bottom: 5px;\r\n}\r\n.category {\r\n    overflow-y: scroll;\r\n    height: 100%;\r\n    width: 76px;\r\n    background: #f5f5f5;\r\n    &_item {\r\n        line-height: 4px;\r\n        text-align: center;\r\n        font-size: 14px;\r\n        color: $content-fontcolor;\r\n        &--active {\r\n            background: $bg_color;\r\n        }\r\n    }\r\n}\r\n.product {\r\n    overflow-y: scroll;\r\n    flex: 1;\r\n    &_item {\r\n        position: relative;\r\n        display: flex;\r\n        padding: 12px 0;\r\n        margin: 0 16px;\r\n        border-bottom: 1px solid $content-bgColor;\r\n        &_detail {\r\n            overflow: hidden;\r\n        }\r\n        &_img {\r\n            width: 75px;\r\n            height: 75px;\r\n            margin-right: 16px;\r\n        }\r\n        &_title {\r\n            line-height: 2px;\r\n            font-size: 14px;\r\n            color: $content-fontcolor;\r\n            @include ellipsis;\r\n        }\r\n        &_sales {\r\n            margin: 6px 0;\r\n            font-size: 12px;\r\n            color: $content-fontcolor;\r\n        }\r\n        &_price {\r\n            margin: 0;\r\n            line-height: 20px;\r\n            font-size: 14px;\r\n            color: $highlight-fontColor;\r\n        }\r\n        &_yen {\r\n            font-size: 12px;\r\n        }\r\n        &_origin {\r\n            margin-left: 6px;\r\n            line-height: 2px;\r\n            font-size: 12px;\r\n            color: $light-fontColor;\r\n            text-decoration: line-through;\r\n        }\r\n        .product_number {\r\n            position: absolute;\r\n            right: 0;\r\n            bottom: 12px;\r\n            &_minus,\r\n            &_plus {\r\n                display: inline-block;\r\n                width: 20px;\r\n                height: 20px;\r\n                line-height: 16px;\r\n                border-radius: 50%;\r\n                font-size: 20px;\r\n                text-align: center;\r\n            }\r\n            &_minus {\r\n                border: 1px solid $medium-fontColor;\r\n                color: $medium-fontColor;\r\n                margin-right: 5px;\r\n            }\r\n            &_plus {\r\n                background: $btn-bgColor;\r\n                color: $bg_color;\r\n                margin-left: 5px;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n"],"mappings":";;;EACSA,KAAK,EAAC;AAAS;;EACZA,KAAK,EAAC;AAAU;mBAF5B;;EAUYA,KAAK,EAAC;AAAS;mBAV3B;;EAeiBA,KAAK,EAAC;AAAqB;;EACxBA,KAAK,EAAC;AAAoB;;EAC3BA,KAAK,EAAC;AAAoB;;EAC1BA,KAAK,EAAC;AAAoB;iEACzBC,mBAAA,CAA2C;EAArCD,KAAK,EAAC;AAAkB,GAAC,GAAK;;EAC9BA,KAAK,EAAC;AAAqB;iEAGzCC,mBAAA,CAIM;EAJDD,KAAK,EAAC;AAAgB,I,aACvBC,mBAAA,CAA2C;EAArCD,KAAK,EAAC;AAAsB,GAAC,GAAC,G,aAxBpDE,gBAAA,CAwB2D,KAE3C,G,aAAAD,mBAAA,CAA0C;EAApCD,KAAK,EAAC;AAAqB,GAAC,GAAC,E;;uBAzB/CG,mBAAA,CA6BM,OA7BNC,UA6BM,GA5BHH,mBAAA,CAOM,OAPNI,UAOM,I,kBANHF,mBAAA,CAIoBG,SAAA,QAP9BC,WAAA,CAIyBC,IAAA,CAAAC,UAAU,EAAlBC,IAAI;yBADXP,mBAAA,CAIoB;MAJdH,KAAK,EAHrBW,eAAA;QAAA,iBAGyCH,IAAA,CAAAI,IAAI;QAAA,yBAA2BJ,IAAA,CAAAK,UAAU,KAAKH,IAAI,CAACI;MAAG;MAEpFC,GAAG,EAAEL,IAAI,CAACM,IAAI;MACdC,OAAK,EAAAC,MAAA,IAAEV,IAAA,CAAAW,mBAAmB,CAACT,IAAI,CAACI,GAAG;wBACjCJ,IAAI,CAACM,IAAI,gCAPtBI,UAAA;oCAUOnB,mBAAA,CAmBM,OAnBNoB,UAmBM,I,kBAlBHlB,mBAAA,CAiBMG,SAAA,QA5BhBC,WAAA,CAYyBC,IAAA,