{"ast":null,"code":"import { reactive, ref, toRefs, watchEffect } from 'vue';\nimport { useRoute } from 'vue-router';\nimport { get } from '../../utils/request';\nconst categories = [{\n  name: '全部商品',\n  tab: 'all'\n}, {\n  name: '秒杀',\n  tab: 'seckill'\n}, {\n  name: '新鲜水果',\n  tab: 'fruit'\n}];\nconst useTabEffect = () => {\n  const currentTab = ref(categories[0].tab);\n  const handleTabClick = tab => {\n    currentTab.value = tab;\n  };\n  return {\n    currentTab,\n    handleTabClick\n  };\n};\nconst useCurrentListEffect = () => {\n  const content = reactive({\n    list: []\n  });\n  const getContentData = async tab => {\n    const result = await get(`/api/shop/${route.params.id}/products`, {\n      tab\n    });\n    if (result?.errno === 0 && result?.data) {\n      data.contentList = result.data;\n    }\n  };\n  watchEffect(() => {\n    getContentData(currentTab.value);\n  });\n};\nexport default {\n  name: 'Content',\n  setup() {\n    const {\n      currentTab,\n      handleTabClick\n    } = useTabEffect();\n    const route = useRoute();\n    const handleCategoryClick = tab => {\n      getContentData(tab);\n    };\n    getContentData('all');\n    const {\n      contentList\n    } = toRefs(data);\n    return {\n      categories,\n      currentTab,\n      handleTabClick,\n      contentList\n    };\n  }\n};","map":{"version":3,"names":["reactive","ref","toRefs","watchEffect","useRoute","get","categories","name","tab","useTabEffect","currentTab","handleTabClick","value","useCurrentListEffect","content","list","getContentData","result","route","params","id","errno","data","contentList","setup","handleCategoryClick"],"sources":["D:\\Besktop\\jingdong\\src\\views\\shop\\Content.vue"],"sourcesContent":["<template>\r\n    <div class=\"content\">\r\n       <div class=\"category\">\r\n          <div :class=\"{'category_item': tuue, 'category_item--active': currentTab === item.tab}\"\r\n          v-for=\"item in categories\"\r\n          :key=\"item.name\"\r\n          @click=\"handleCategoryClick(item.tab)\"\r\n          >{{item.name}}</div>\r\n\r\n       </div>\r\n       <div class=\"product\">\r\n          <div class=\"product_item\"\r\n          v-for=\"item in contentList\"\r\n          :key=\"item._id\">\r\n            <img class=\"product_item_img\" src=\"http://www.dell-lee.com/imgs/vue3/near.png\" alt=\"\">\r\n            <div class=\"product_item_detail\">\r\n                <h4 class=\"product_item_title\">{{item.name}}</h4>\r\n                <p class=\"product_item_sales\">月售 {{ item.sales }}</p>\r\n                <p class=\"product_item_price\">\r\n                    <span class=\"product_item_yen\">&yen;</span>{{item.price}}\r\n                    <span class=\"product_item_origin\">&yen;{{ item.oldPrice }}</span>\r\n                </p>\r\n            </div>\r\n            <div class=\"product_number\">\r\n                <span class=\"product_number_minus\">-</span>\r\n                0\r\n                <span class=\"product_number_plus\">+</span>\r\n            </div>\r\n          </div>\r\n       </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, ref, toRefs, watchEffect } from 'vue'\r\nimport { useRoute } from 'vue-router'\r\nimport { get } from '../../utils/request'\r\n\r\nconst categories = [\r\n  { name: '全部商品', tab: 'all' },\r\n  { name: '秒杀', tab: 'seckill' },\r\n  { name: '新鲜水果', tab: 'fruit' }\r\n]\r\n\r\nconst useTabEffect = () => {\r\n  const currentTab = ref(categories[0].tab)\r\n  const handleTabClick = (tab) => {\r\n    currentTab.value = tab\r\n  }\r\n  return { currentTab, handleTabClick }\r\n}\r\n\r\nconst useCurrentListEffect = () => {\r\n  const content = reactive({ list: [] })\r\n\r\n  const getContentData = async (tab) => {\r\n    const result = await get(`/api/shop/${route.params.id}/products`, { tab })\r\n    if (result?.errno === 0 && result?.data) {\r\n        data.contentList = result.data\r\n      }\r\n    }\r\n\r\n  watchEffect(() => {\r\n    getContentData(currentTab.value)\r\n  })\r\n}\r\n\r\nexport default {\r\n  name: 'Content',\r\n  setup () {\r\n    const { currentTab, handleTabClick } = useTabEffect()\r\n\r\n    const route = useRoute()\r\n\r\n    const handleCategoryClick = (tab) => {\r\n      getContentData(tab)\r\n    }\r\n\r\n    getContentData('all')\r\n    const { contentList } = toRefs(data)\r\n\r\n    return { categories, currentTab, handleTabClick, contentList }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables';\r\n@import '../../style/mixins.scss';\r\n.content {\r\n    display: flex;\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n    top: 15px;\r\n    bottom: 5px;\r\n}\r\n.category {\r\n    overflow-y: scroll;\r\n    height: 100%;\r\n    width: 76px;\r\n    background: #f5f5f5;\r\n    &_item {\r\n        line-height: 4px;\r\n        text-align: center;\r\n        font-size: 14px;\r\n        color: $content-fontcolor;\r\n        &--active {\r\n            background: $bg_color;\r\n        }\r\n    }\r\n}\r\n.product {\r\n    overflow-y: scroll;\r\n    flex: 1;\r\n    &_item {\r\n        position: relative;\r\n        display: flex;\r\n        padding: 12px 0;\r\n        margin: 0 16px;\r\n        border-bottom: 1px solid $content-bgColor;\r\n        &_detail {\r\n            overflow: hidden;\r\n        }\r\n        &_img {\r\n            width: 75px;\r\n            height: 75px;\r\n            margin-right: 16px;\r\n        }\r\n        &_title {\r\n            line-height: 2px;\r\n            font-size: 14px;\r\n            color: $content-fontcolor;\r\n            @include ellipsis;\r\n        }\r\n        &_sales {\r\n            margin: 6px 0;\r\n            font-size: 12px;\r\n            color: $content-fontcolor;\r\n        }\r\n        &_price {\r\n            margin: 0;\r\n            line-height: 20px;\r\n            font-size: 14px;\r\n            color: $highlight-fontColor;\r\n        }\r\n        &_yen {\r\n            font-size: 12px;\r\n        }\r\n        &_origin {\r\n            margin-left: 6px;\r\n            line-height: 2px;\r\n            font-size: 12px;\r\n            color: $light-fontColor;\r\n            text-decoration: line-through;\r\n        }\r\n        .product_number {\r\n            position: absolute;\r\n            right: 0;\r\n            bottom: 12px;\r\n            &_minus,\r\n            &_plus {\r\n                display: inline-block;\r\n                width: 20px;\r\n                height: 20px;\r\n                line-height: 16px;\r\n                border-radius: 50%;\r\n                font-size: 20px;\r\n                text-align: center;\r\n            }\r\n            &_minus {\r\n                border: 1px solid $medium-fontColor;\r\n                color: $medium-fontColor;\r\n                margin-right: 5px;\r\n            }\r\n            &_plus {\r\n                background: $btn-bgColor;\r\n                color: $bg_color;\r\n                margin-left: 5px;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AAkCA,SAASA,QAAQ,EAAEC,GAAG,EAAEC,MAAM,EAAEC,WAAU,QAAS,KAAI;AACvD,SAASC,QAAO,QAAS,YAAW;AACpC,SAASC,GAAE,QAAS,qBAAoB;AAExC,MAAMC,UAAS,GAAI,CACjB;EAAEC,IAAI,EAAE,MAAM;EAAEC,GAAG,EAAE;AAAM,CAAC,EAC5B;EAAED,IAAI,EAAE,IAAI;EAAEC,GAAG,EAAE;AAAU,CAAC,EAC9B;EAAED,IAAI,EAAE,MAAM;EAAEC,GAAG,EAAE;AAAQ,EAC/B;AAEA,MAAMC,YAAW,GAAIA,CAAA,KAAM;EACzB,MAAMC,UAAS,GAAIT,GAAG,CAACK,UAAU,CAAC,CAAC,CAAC,CAACE,GAAG;EACxC,MAAMG,cAAa,GAAKH,GAAG,IAAK;IAC9BE,UAAU,CAACE,KAAI,GAAIJ,GAAE;EACvB;EACA,OAAO;IAAEE,UAAU;IAAEC;EAAe;AACtC;AAEA,MAAME,oBAAmB,GAAIA,CAAA,KAAM;EACjC,MAAMC,OAAM,GAAId,QAAQ,CAAC;IAAEe,IAAI,EAAE;EAAG,CAAC;EAErC,MAAMC,cAAa,GAAI,MAAOR,GAAG,IAAK;IACpC,MAAMS,MAAK,GAAI,MAAMZ,GAAG,CAAC,aAAaa,KAAK,CAACC,MAAM,CAACC,EAAE,WAAW,EAAE;MAAEZ;IAAI,CAAC;IACzE,IAAIS,MAAM,EAAEI,KAAI,KAAM,KAAKJ,MAAM,EAAEK,IAAI,EAAE;MACrCA,IAAI,CAACC,WAAU,GAAIN,MAAM,CAACK,IAAG;IAC/B;EACF;EAEFnB,WAAW,CAAC,MAAM;IAChBa,cAAc,CAACN,UAAU,CAACE,KAAK;EACjC,CAAC;AACH